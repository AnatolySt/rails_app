<%= form_for(admin_railway_stations_path) do |form| %>
    <%= render 'common/errors', resource: @railway_station %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title, id: :railway_station_title %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
